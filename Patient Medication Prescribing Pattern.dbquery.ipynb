   "source": [
    "-- 10 Medications that are prescribed most often and for what conditions\n",
    "SELECT m.description AS medication, COUNT(*) AS medication_prescribed\n",
    "FROM medications m\n",
    "GROUP BY m.description\n",
    "ORDER BY medication_prescribed DESC\n",
    "LIMIT 10;"
